<?xml version="1.0"?>
<rdf:RDF xmlns="http://purl.org/andreapoltronieri/music-annotation-pattern/"
     xml:base="http://purl.org/andreapoltronieri/music-annotation-pattern/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/">
        <owl:versionIRI rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/0.1/"/>
        <dc:creator xml:lang="en">Andrea Poltronieri</dc:creator>
        <dc:creator xml:lang="en">Jacopo de Berardinis</dc:creator>
        <dc:creator xml:lang="en">Valentina Presutti</dc:creator>
        <dc:license rdf:resource="https://creativecommons.org/licenses/by-sa/4.0/"/>
        <terms:license rdf:resource="https://creativecommons.org/licenses/by-sa/4.0/"/>
        <rdfs:comment xml:lang="en">An Ontology Design Pattern (ODP) for modelling differnt types of musical annotation</rdfs:comment>
        <rdfs:label xml:lang="en">Music Annotation Pattern</rdfs:label>
        <owl:versionInfo xml:lang="en">1.0</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/dc/elements/1.1/creator -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/creator"/>
    


    <!-- http://purl.org/dc/elements/1.1/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/license"/>
    


    <!-- http://purl.org/dc/elements/1.1/title -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/elements/1.1/title"/>
    


    <!-- http://purl.org/dc/terms/license -->

    <owl:AnnotationProperty rdf:about="http://purl.org/dc/terms/license"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/annotationValidFrom -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/annotationValidFrom">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMetricalTimeOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MetricalTime"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotator"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotatorType"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasJAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/includesObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf"/>
        <rdfs:comment xml:lang="en">Relates a JAMS Score Annotation to its metrical information, expressed by the MetricalTime class.</rdfs:comment>
        <rdfs:label xml:lang="en">Annotation Valid From</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotator -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotator">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotation"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotatorType"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasJAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/includesObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf"/>
        <rdfs:comment xml:lang="en">Links a JAMS annotation to the Annotator the annotated the JAMS.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Annotator</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotatorType -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasAnnotatorType">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/AnnotatorType"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasJAMSAnnotation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/includesObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf"/>
        <rdfs:comment xml:lang="en">Links the Annotator class to its Annotator Type, describing how the JAMS annotations were generated.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Annotator Type</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasJAMSAnnotation -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasJAMSAnnotation">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotationOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/includesObservation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf"/>
        <rdfs:comment xml:lang="en">Relates a Score or a Recording to the JAMS annotation describing the track.</rdfs:comment>
        <rdfs:label xml:lang="en">Has JAMS Annotation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/includesObservation -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/includesObservation">
        <owl:inverseOf rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#InverseFunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotation"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Observation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotationOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf"/>
        <rdfs:comment xml:lang="en">Links a JAMS Annotation to the JAMS Onservation(s) it contains.</rdfs:comment>
        <rdfs:label xml:lang="en">Includes Observation</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotationOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotationOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf"/>
        <rdfs:comment xml:lang="en">Inverse of hasJAMSAnnotation. It links the JAMS Annotation to either a Recording o a Score.</rdfs:comment>
        <rdfs:label xml:lang="en">Is JAMS Annotation Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorOf">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotation"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf"/>
        <rdfs:comment xml:lang="en">Inverse of hasAnnotator. Links the Annotator to the JAMS Annotation annotated by such annotator.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Annotator Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isAnnotatorTypeOf">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/AnnotatorType"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isMetricalTimeOf"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf"/>
        <rdfs:comment xml:lang="en">Inverse of hasAnnotatorType. Links the Annotator Type to the Annotator class.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Annotator Type Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isMetricalTimeOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isMetricalTimeOf">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MetricalTime"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf"/>
        <rdfs:comment xml:lang="en">Inverse of startsAt. It relates the Metrical Time of a JAMS Score Observation, expressing the score observation&apos;s timing information.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Metrical Time Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf -->

    <owl:ObjectProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/isObservationOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Observation"/>
        <rdfs:range rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotation"/>
        <rdfs:comment xml:lang="en">Inverse of includesObservation. Links a JAMS Observation to the JAMS Annotation to which it belongs.</rdfs:comment>
        <rdfs:label xml:lang="en">Is Observation Of</rdfs:label>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasBeat -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasBeat">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MetricalTime"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasConfidence"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasDurartion"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasMeasure"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasName"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValdityStartingTime"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValidityDuration"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/startsAtTime"/>
        <rdfs:comment xml:lang="en">The starting beat of an annotation or an observation within a measure.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Beat</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasConfidence -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasConfidence">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasDurartion"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasMeasure"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasName"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValdityStartingTime"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValidityDuration"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/startsAtTime"/>
        <rdfs:comment xml:lang="en">The confidence of an annotation/observation, as described by the JAMS documentation, hence expressed with a floating number from 0 to 1.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Confidence</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasDurartion -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasDurartion">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Observation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasMeasure"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasName"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValdityStartingTime"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValidityDuration"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/startsAtTime"/>
        <rdfs:comment xml:lang="en">The duration of an observation, either expressing real time or symbolic time.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Duration</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasMeasure -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasMeasure">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/MetricalTime"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#nonNegativeInteger"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasName"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValdityStartingTime"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValidityDuration"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/startsAtTime"/>
        <rdfs:comment xml:lang="en">The measure in which the observation or the annotation takes place.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Measure</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasName -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasName">
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotator"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValdityStartingTime"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValidityDuration"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/startsAtTime"/>
        <rdfs:comment xml:lang="en">The name of an annotator, which can be either human, artificial (e.g. the name of a software) or describing a group of people (e.g. the name of a crowdsourcing service/event).</rdfs:comment>
        <rdfs:label xml:lang="en">Has Name</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasValdityStartingTime -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValdityStartingTime">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/AudioAnnotation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValidityDuration"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/startsAtTime"/>
        <rdfs:comment xml:lang="en">The start time of an Audio Annotation, thus expressed in seconds.</rdfs:comment>
        <rdfs:label xml:lang="en">Has Validity Starting Time</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/hasValidityDuration -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/hasValidityDuration">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <owl:propertyDisjointWith rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/startsAtTime"/>
        <rdfs:comment xml:lang="en">The duration of an annotation, whether audio or score.</rdfs:comment>
        <rdfs:label xml:lang="en">HasValidity Duration</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/startsAtTime -->

    <owl:DatatypeProperty rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/startsAtTime">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://purl.org/andreapoltronieri/music-annotation-pattern/AudioObservation"/>
        <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#float"/>
        <rdfs:comment xml:lang="en">The start time of an observation belonging to an &apos;audio&apos; type annotation, and thus expressed in seconds.</rdfs:comment>
        <rdfs:label xml:lang="en">Starts At Time</rdfs:label>
    </owl:DatatypeProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/andreapoltronieri/music-annotation-pattern/Annotation -->

    <owl:Class rdf:about="http://purl.org/andreapoltronieri/music-annotation-pattern/Annotation">
        <rdfs:subClassOf>
            <owl:Restriction>
